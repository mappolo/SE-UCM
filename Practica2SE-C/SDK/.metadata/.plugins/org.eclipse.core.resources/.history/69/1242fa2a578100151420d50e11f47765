# Add inputs and outputs from these tool invocations to the build variables 
ELFSIZE += \
P2SE-A.elf.size \

ELFCHECK += \
P2SE-A.elf.elfcheck \


# All Target
all: P2SE-A.elf secondary-outputs

# Tool invocations
P2SE-A.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo Building target: $@
	@echo Invoking: MicroBlaze gcc linker
	mb-gcc -Wl,--no-relax -Wl,-T -Wl,../src/lscript.ld -L../../peripheral_tests_bsp_0/microblaze_0/lib -mxl-pattern-compare -mcpu=v8.30.a -mno-xl-soft-mul -o"P2SE-A.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo Finished building target: $@
	@echo ' '

P2SE-A.elf.size: P2SE-A.elf
	@echo Invoking: MicroBlaze Print Size
	mb-size P2SE-A.elf  |tee "P2SE-A.elf.size"
	@echo Finished building: $@
	@echo ' '

P2SE-A.elf.elfcheck: P2SE-A.elf
	@echo Invoking: Xilinx ELF Check
	elfcheck P2SE-A.elf -hw ../../Practica2SE_hw_platform/system.xml -pe microblaze_0  |tee "P2SE-A.elf.elfcheck"
	@echo Finished building: $@
	@echo ' '

